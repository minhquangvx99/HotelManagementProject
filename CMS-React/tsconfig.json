{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "components": [
        "./components"
      ],
      "config": [
        "./config"
      ],
      "layout": [
        "./layout"
      ],
      "routes": [
        "./routes"
      ],
      "types": [
        "./types"
      ],
      "store": [
        "./store"
      ],
      "utility": [
        "./utility"
      ],
      "static": [
        "./static"
      ],
      "services": [
        "./services"
      ],
      "container": [
        "./container"
      ],
    },
  },
  "include": [
    "src",
  ]
}